# Hedera Configuration - Agent Payment Receiver
HEDERA_ACCOUNT_ID=0.0.YOUR_ACCOUNT_ID
HEDERA_PRIVATE_KEY=0xYOUR_PRIVATE_KEY
HEDERA_NETWORK=testnet
AGENT_EVM_ADDRESS=0xYOUR_EVM_ADDRESS

# Pinata IPFS Configuration
PINATA_API_KEY=your_pinata_api_key
PINATA_API_SECRET=your_pinata_api_secret

# AI Model Provider (OpenAI only)
OPENAI_API_KEY=sk-proj-YOUR_OPENAI_API_KEY

# Fastify Server Configuration
FASTIFY_PORT=10000

# ngrok Configuration
# Get your authtoken from: https://dashboard.ngrok.com/get-started/your-authtoken
NGROK_AUTHTOKEN=your_ngrok_authtoken

# Database Configuration
DATABASE_URL=file:./dev.db

# Orchestrator Configuration - Payment Sender
ORCHESTRATOR_ACCOUNT_ID=0.0.YOUR_ORCHESTRATOR_ACCOUNT_ID
ORCHESTRATOR_EVM_ADDRESS=0xYOUR_ORCHESTRATOR_EVM_ADDRESS
ORCHESTRATOR_PRIVATE_KEY=0xYOUR_ORCHESTRATOR_PRIVATE_KEY

# Facilitator Configuration - X402 Payment Facilitator
FACILITATOR_ACCOUNT_ID=0.0.YOUR_FACILITATOR_ACCOUNT_ID
FACILITATOR_EVM_ADDRESS=0xYOUR_FACILITATOR_EVM_ADDRESS
FACILITATOR_PRIVATE_KEY=0xYOUR_FACILITATOR_PRIVATE_KEY

# Encryption Configuration - For encrypting judge private keys
# Generate using: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENCRYPTION_KEY=your_32_byte_hex_encryption_key

# Backend URL Configuration
BACKEND_URL=http://localhost:10000